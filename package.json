	},
	"keywords": [
		"ftp",
        "ftp file manager",
        "ftp client",
		"viewer",
		"browser",
        "gui",
		"file",
        "syncronization manager"
	],
	"dependencies": {
		"colors": "*",
		"express": "3.5.x",
        "ftpimp": "1.0.x",
        "winston": "0.9.x",
		"express-handlebars": "1.2.x"
	},
	"engine": {
		"node": ">=0.10.x"
	},
	"author": {
	 "name": "Nick Riley",
